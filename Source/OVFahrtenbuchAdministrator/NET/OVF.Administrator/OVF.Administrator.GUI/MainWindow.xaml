<Window x:Name="Administrator" x:Class="OVF.Administrator.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OVF.Administrator.GUI"
        mc:Ignorable="d"
        Title="Fahrtenbuch Administrator" Height="150.302" Width="500.302">
    <Grid x:Name="Anmeldung" Margin="0,-1,0,1">
        <Label Content="Fahrtenbuch Login" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top" Width="270" FontFamily="Arial Black" FontSize="14"/>
        <Label x:Name="lblPassword1" Content="Passwort" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top" Width="109"/>
        <Label x:Name="lblPassword2" Content="Passwort wiederholen" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top" Width="125"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="354,41,0,0" VerticalAlignment="Top" Width="126" Click="btnLogin_Click" Height="22"/>
        <PasswordBox x:Name="txtPassword1" HorizontalAlignment="Left" Margin="134,41,0,0" VerticalAlignment="Top" Width="215" Height="22"/>
        <PasswordBox x:Name="txtPassword2" HorizontalAlignment="Left" Margin="134,69,0,0" VerticalAlignment="Top" Width="215" Height="22"/>

    </Grid>
</Window>
